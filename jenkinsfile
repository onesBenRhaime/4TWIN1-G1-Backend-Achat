pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Clean') {
            steps {
                sh 'rm -rf target/*'
            }
        }

        stage('MAVEN BUILD') {
            steps {
                sh 'mvn clean compile'
            }
        }

       stage('MOCKITO') {
             steps {
                 sh 'mvn test'
             }
       }
       stage('PACKAGE') {
          steps {
             sh 'mvn package'
          }
       }

    stage("MVN SONARQUBE") {
    steps {
        withSonarQubeEnv(installationName: 'sonar') {
            sh "mvn -DskipTests sonar:sonar \
                -Dsonar.projectKey=4TWIN1-G1-Achat \
                -Dsonar.branch.name=eyamechergui-4TWIN1-G1"
        }
      }
    }

    }
}
