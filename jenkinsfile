pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Maven Clean Compile') {
                  steps {
                      sh 'mvn clean'
                      echo 'Running Maven Compile'
                      sh 'mvn compile'
                  }
        }

       stage('Tests - JUnit/Mockito') {
             steps {
                 sh 'mvn test'
             }
       }
       stage('Build package') {
            steps {
                sh 'mvn package'
            }
       }
       stage('Maven Install') {
            steps {
                sh 'mvn install'
            }
       }

    }
}